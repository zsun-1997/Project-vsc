{
    "name": "expressts",
    "version": "0.0.1",
    "description": "Express Scaffolding Project",
    "main": "src/server.ts",
    "scripts": {
        "migration:generate": "npm run build && npm run typeorm migration:generate -- -n",
        "build": "tsc",
        "clean": "rm -r build",
        "coverage": "nyc ts-mocha tests/*.ts --exit",
        "lint": "eslint ./{seeds,src,tests}/{*,**}.ts",
        "lint:fix": "eslint ./{seeds,src,tests}/{*,**}.ts --fix",
        "seed": "ts-node-dev db/seeds/seeds.ts",
        "start": "ts-node-dev --respawn --transpile-only --ignore-watch node_modules --no-notify src/server.ts",
        "test": "ts-mocha tests/*.ts --exit",
        "typeorm": "ts-node-dev --transpile-only ./node_modules/typeorm/cli.js"
    },
    "author": "",
    "dependencies": {
        "@types/body-parser": "^1.19.2",
        "@types/cors": "^2.8.6",
        "@types/debug": "^4.1.5",
        "@types/express": "^4.17.13",
        "@types/express-session": "^1.17.0",
        "@types/helmet": "0.0.45",
        "@types/jsonwebtoken": "^8.5.0",
        "@types/morgan": "^1.9.1",
        "@types/node": "^13.13.52",
        "@types/swagger-jsdoc": "^3.0.2",
        "@types/uuid": "^7.0.4",
        "body-parser": "^1.19.1",
        "boolean": "^3.0.1",
        "cors": "^2.8.5",
        "csv-parser": "^3.0.0",
        "debug": "^4.1.1",
        "dotenv": "^8.2.0",
        "express": "^4.17.2",
        "express-async-handler": "^1.1.4",
        "express-session": "^1.17.1",
        "helmet": "^3.23.1",
        "jsonwebtoken": "^8.5.1",
        "morgan": "^1.10.0",
        "morgan-debug": "^2.0.0",
        "mysql": "^2.18.1",
        "neat-csv": "^7.0.0",
        "nyc": "^15.1.0",
        "pg": "^7.18.2",
        "reflect-metadata": "^0.1.13",
        "swagger-jsdoc": "^3.7.0",
        "ts-node-dev": "^1.0.0-pre.49",
        "typeorm": "^0.2.41",
        "typescript": "^3.9.10",
        "uuid": "^7.0.3"
    },
    "devDependencies": {
        "@types/chai": "^4.2.11",
        "@types/chai-as-promised": "^7.1.2",
        "@types/mocha": "^7.0.2",
        "@typescript-eslint/eslint-plugin": "^5.9.0",
        "@typescript-eslint/parser": "^5.9.0",
        "chai": "^4.2.0",
        "chai-as-promised": "^7.1.1",
        "chai-http": "^4.3.0",
        "eslint": "^8.6.0",
        "eslint-config-airbnb-base": "^15.0.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-chai-friendly": "^0.5.0",
        "eslint-plugin-import": "^2.25.4",
        "mocha": "^8.0.1",
        "prettier": "2.5.1",
        "ts-mocha": "^6.0.0"
    }
}
